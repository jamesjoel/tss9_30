<!DOCTYPE html>
<html>
<head ng-app="myApp">
	<title></title>
	<link rel="stylesheet" type="text/css" href="/css/bootstrap.css">
	<link rel="stylesheet" type="text/css" href="/css/style.css">
	<script type="text/javascript" src="/js/jquery.js"></script>
	<script type="text/javascript" src="/js/bootstrap.js"></script>
	<script type="text/javascript" src="/js/angular.js"></script>
	<script type="text/javascript" src="/js/app.js"></script>
</head>
<body ng-controller="loginCtrl">
	<div class="container">
		<div class="col-md-4 col-xs-4 col-sm-4 col-lg-4 col-md-offset-4 col-sm-offset-4 col-xs-offset-4 col-lg-offset-4">
				<div class="panel-heading">
					<h3> Login </h3>
				</div>
				<div class="panel-body">
					<img src="/images/avatar.png" height="50" width="50"/> <br/> <br/>
					<input ng-model="loginData.username" type="text" placeholder="Username/Email"  autofocus> <br/> <br/>
					<!-- <input type="password" ng-model="loginData.password" placeholder="Password" class="form-control"> <br/> -->
					<button class="btn btn-large btn-primary" type="button" ng-click="login()"> Next </button> <br/><br/>
					<!-- <img ng-show="preloaderShow" src="/images/5.gif" height="15" width="15"/> <br/>  --><br/>
					<p ng-bind="msg" class="text-danger text-center"> </p>
				</div>
		</div>
	</div>
</body>
</html>