<!DOCTYPE html>
<html>
<head>
	<title></title>
</head>
<body>
<h1>This is home Page</h1>
<table align="center" border="1">
	<tr>
		<th>S.No.</th>
		<th>Name</th>
		<th>City</th>
		<th>Age</th>
	</tr>
	<% arr.forEach(function(item, index){ %>
		<tr>
			<td><%= index+1 %></td>
			<td><%= item.full_name %></td>
			<td><%= item.city %></td>
			<td><%= item.age %></td>
		</tr>
	<% }) %>
</table>
</body>
</html>