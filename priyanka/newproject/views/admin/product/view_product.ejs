<h1> View All Products </h1>
        <table class="table-hover table-stripped">
                <tr>
                    <th>S.No.</th>
                    <th>Product Name</th>
                    <th>Product Price</th>
                    <th>Detail</th>
                    <th>Discount Price</th>
                </tr>
                <%result.forEach(function(item,index)
                {%>
                    <tr>
                        <td><%= index+1 %></td>
                        <td><% = item.name%></td>
                        <td><% = item.price%></td>
                        <td><% = item.detail%></td>
                        <td><% = item.discount%></td>
                        <td><a href="#" class="btn btn-sm btn-info">Edit</a></td>
            <td><a href="#"  class="btn btn-sm btn-danger">Delete</a></td>
                    </tr>
                <% }) %>
                </table>
