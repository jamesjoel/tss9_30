<div class="container">
	<table class="table table-hover table-striped tab;e-bordered">
		<tr>
			<th> S.No. </th>
			<th> Full Name </th>
			<th> Age </th>
			<th> Fee </th>
			<th> Edit </th>
			<th> Star </th>
			<th> Delete </th>
		</tr>
		<% 
		result.forEach(function(item,index){ %>
			<tr>
				<td> <%= index+1 %> </td>	
				<td> <%= item.full_name %> </td>	
				<td> <%= item.age %> </td>	
				<td> <%= item.fee %> </td>	
				<td> <a href="/edit/<%= item._id %>"> Edit </a></td>

		<td>		

		<% var id=item._id; 
		if(starid==id){ %>
			<a href="/view_cookie/c_create/<%= item._id%>" style="background-color: yellow;"><span class="glyphicon glyphicon-star"></span></a>
		<% } else { %>
			 <a href="/view"><span class="glyphicon glyphicon-star"></span></a> 
		<% } %>		
			</td>	
			<td> <a href="/view_cookie/delete/<%= item._id%>"> Delete </a></td>
		<% }) %>
	</tr>
	</table>
</div>