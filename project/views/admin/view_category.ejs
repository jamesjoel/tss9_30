<h1>View All Category</h1>
<table class="table table-bordered table-hover table-stripped">
	<tr>
		<th>S.No.</th>
		<th>Categroy Name</th>
		<th>Edit</th>
		<th>Delete</th>
	</tr>
	<% result.forEach(function(item, index){ %>
		<tr>
			<td><%= index+1 %></td>
			<td><%= item.name %></td>
			<td><a href="/admin/category/edit/<%= item._id %>" class="btn btn-sm btn-info">Edit</a></td>
			<td><a href="/admin/category/delete/<%= item._id %>" class="btn btn-sm btn-danger">Delete</a></td>
		</tr>
	<% }) %>
</table>